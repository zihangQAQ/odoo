<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="子航_list">
      <field name="name">主播销售数据</field>
      <field name="model">zhubo.xiaoshoushuju</field>

      <field name="arch" type="xml">
          <tree editable="bottom">
              <field name="name"/>
              <field name="date"/>
              <field name="xiaoshoue"/>
              <field name="name1"/>
              <field name="name2"/>
          </tree>
      </field>
    </record>







    <record model="ir.ui.view" id="子航_search">
      <field name="name">主播查询</field>
      <field name="model">zhubo.xiaoshoushuju</field>
      <field name="arch" type="xml">
      <search string="主播销售数据">
          <field name="name"/>
          <field name="date"/>

          <field name="name1"/>
      </search>
      </field>
    </record>


    <!-- actions opening views on models -->





    <record model="ir.ui.view" id="zhubofenlei_list">
      <field name="name">主播分类</field>
      <field name="model">zhubo.fenlei</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>

        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="子航_act_window">
      <field name="name">主播销售数据</field>
      <field name="res_model">zhubo.xiaoshoushuju</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.actions.act_window" id="zhubofenlei_act_window">
      <field name="name">主播分类</field>
      <field name="res_model">zhubo.fenlei</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.actions.act_window" id="Anchor_information_act_window">
      <field name="name">主播信息</field>
      <field name="res_model">anchor.information</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.actions.act_window" id="Anchor_sales_data_act_window">
      <field name="name">销售数据</field>
      <field name="res_model">anchor.sales_data</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->
<!--
    <record model="ir.actions.server" id="子航.action_server">
      <field name="name">子航 server</field>
      <field name="model_id" ref="model_子航_子航"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>
-->

    <!-- Top menu item -->

    <menuitem name="销售运营部" id="子航_menu_root" groups = "base.group_user" sequence = "10"/>
    <menuitem name="主播销售数据" id="sec_子航_menu"
              parent = "子航_menu_root"
              action="子航_act_window"
              sequence = "10"/>

    <menuitem name="主播分类" id="sec_zhubofenlei_menu_1"
              parent = "子航_menu_root"/>
    <menuitem name="主播分类" id="sec_zhubofenlei_menu_2"
              parent = "sec_zhubofenlei_menu_1"
              action="zhubofenlei_act_window"/>
    <menuitem name="主播信息" id="sec_Anchor_information_menu_1"
              parent="子航_menu_root"/>
    <menuitem name="主播信息" id="sec_Anchor_information_menu_2"
              parent="sec_Anchor_information_menu_1"
              action="Anchor_information_act_window"/>
    <menuitem name="销售数据" id="sec_Anchor_sales_data_menu_1"
              parent="子航_menu_root"
              action="Anchor_sales_data_act_window"/>


    <!-- menu categories -->


    <!-- actions -->
