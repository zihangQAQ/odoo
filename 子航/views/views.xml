    <menuitem name="List" id="子航.menu_1_list" parent="子航.menu_1"
              action="子航.action_window"/>
    <menuitem name="Server to list" id="子航" parent="子航.menu_2"
              action="子航.action_server"/>
-->
  </data>
</odoo>